{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ingestingdatafromgithub"
		},
		"GitHub1_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub1'"
		},
		"ls_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureBlobStorage'"
		},
		"ls_adf_to_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adf_to_adls'"
		},
		"GitHub1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "naveenkodali3564"
		},
		"Http_raw_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/manojminumula/Ipl_dataset/blob/main/New_data.xlsx"
		},
		"ls_adf_to_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://alebraittargetdata.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ingestingdatafromgithub_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"GitHub1_credential": {
						"value": "[parameters('GitHub1_credential')]"
					},
					"ls_AzureBlobStorage_connectionString": {
						"value": "[parameters('ls_AzureBlobStorage_connectionString')]"
					},
					"ls_adf_to_adls_accountKey": {
						"value": "[parameters('ls_adf_to_adls_accountKey')]"
					},
					"GitHub1_properties_typeProperties_username": {
						"value": "[parameters('GitHub1_properties_typeProperties_username')]"
					},
					"Http_raw_linked_service_properties_typeProperties_url": {
						"value": "[parameters('Http_raw_linked_service_properties_typeProperties_url')]"
					},
					"ls_adf_to_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adf_to_adls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}